{
    "name": "dsfeventtracker",
    "version": "1.0.0",
    "description": "Front end of the DSF Event Tracker Alt1 plugin.",
    "homepage": "https://www.dsfeventtracker.com/",
    "bugs": {
        "url": "https://github.com/LukeHankey/issues"
    },
    "license": "MIT",
    "author": "Luke Hankey",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LukeHankey/DSF-Event-Tracker.git"
    },
    "engines": {
        "node": ">=23.9.0"
    },
    "scripts": {
        "build": "webpack --env VERSION=alt1 && npm run postbuild",
        "build:dev": "webpack --env VERSION=alt1-dev && npm run postbuild",
        "postbuild": "cp index.md _config.yml README.md favicon.ico CNAME robots.txt package.json dist/ && cp -r faq _includes dist/",
        "watch": "webpack --watch",
        "deploy": "gh-pages -d dist",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "alt1": "^0.1.0",
        "axios": "^1.7.7",
        "fuse.js": "^7.1.0",
        "tesseract.js": "^6.0.1",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@eslint/css": "^0.7.0",
        "@eslint/eslintrc": "^3.3.0",
        "@eslint/js": "^9.26.0",
        "@eslint/json": "^0.12.0",
        "@eslint/markdown": "^6.4.0",
        "@types/node": "^22.15.14",
        "copy-webpack-plugin": "^13.0.0",
        "css-loader": "^7.1.2",
        "eslint": "^9.26.0",
        "express": "^4.21.2",
        "gh-pages": "^6.3.0",
        "globals": "^16.0.0",
        "prettier": "^3.5.2",
        "sharp": "^0.33.5",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.3.1",
        "typescript": "^5.1.3",
        "typescript-eslint": "^8.32.0",
        "webpack": "^5.98.0",
        "webpack-cli": "^6.0.1"
    }
}
